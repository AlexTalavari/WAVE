@using System.Reflection
@model LayoutModel




<!DOCTYPE html>
<!--[if lt IE 7]> <html class="ie lt-ie9 lt-ie8 lt-ie7 app" > <![endif]-->
<!--[if IE 7]>    <html class="ie lt-ie9 lt-ie8 app" > <![endif]-->
<!--[if IE 8]>    <html class="ie lt-ie9 app" > <![endif]-->
<!--[if gt IE 8]> <html class="app" > <![endif]-->
<!--[if !IE]><!-->
<html class="app">
<!-- <![endif]-->
<head>
    <title>WAVE | @Model.Title</title>

    <!-- Meta -->
    <meta charset="UTF-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0, user-scalable=0, minimum-scale=1.0, maximum-scale=1.0">
    <meta name="apple-mobile-web-app-capable" content="yes">
    <meta name="apple-mobile-web-app-status-bar-style" content="black">
    <meta http-equiv="X-UA-Compatible" content="IE=9; IE=8; IE=7; IE=EDGE" />
    <!-- Meta -->

    <link href="~/Content/css/layout.css" rel="stylesheet" />
    <link href="~/Content/css/Coral/picto.css" rel="stylesheet" />
    <link href="~/Scripts/jquery.cookiebar.css" rel="stylesheet" />
    <link href="~/Content/css/Coral/owl.carousel.css" rel="stylesheet" />
    <link href="~/Content/css/Coral/owl.theme.css" rel="stylesheet" />
    <link href="~/Content/css/plugins/tooltipster/tooltipster.css" rel="stylesheet" />

    <link href="~/Content/css/plugins/tooltipster/themes/tooltipster-shadow.css" rel="stylesheet" />
    @RenderSection("Header", false)

    <script src="~/Scripts/Coral/modernizr.js"></script>
    <script src="~/Scripts/Coral/jquery.min.js"></script>
    <link href="~/Content/css/plugins/maxlength/jquery.maxlength.css" rel="stylesheet" />
    <link href="~/Content/css/bootstrap/bootstrap-progressbar-2.3.2.min.css" rel="stylesheet" />
    <link href="~/Content/css/actions.css" rel="stylesheet" />
</head>

<body>



    <!-- Main Container Fluid -->
    <div class="container-fluid">

        @Html.Partial("~/Views/Shared/LayoutPartials/_Menu.cshtml")
        <!-- Content -->
        <div id="content">

            @Html.Partial("~/Views/Shared/LayoutPartials/_Navbar.cshtml", Model)
            @if (Model.NotificationMessage != null && !Model.NotificationMessage.IsEmpty())
            {
                <div id="note">@Model.NotificationMessage</div>
            }
            <p><a href="#" class="notify" style="display: none">@Resources.SharedLayoutNotifyMe</a></p>
            <div id="top"></div>
            <!-- Start of Body -->
            @RenderBody()
            <!-- End of Body -->
            @*<a href="#top" id="gototop" style="font-size: 25px; position: absolute; right: 25px; visibility: visible; z-index: 5000000; ">Go to top</a>*@
        </div>
        <!-- // Content END -->
        <div class="clearfix"></div>
        <!-- // Sidebar menu & content wrapper END -->
        <!-- // Footer END -->


    </div>
    <!-- // Main Container Fluid END -->
    <!-- Global -->
    <script>
        var primaryColor = '#2776ec',
            dangerColor = '#b55151',
            successColor = '#609450',
            warningColor = '#ab7a4b',
            inverseColor = '#45484d';
    </script>
    <script src="~/Scripts/Coral/jquery-migrate.min.js"></script>
    <script src="~/Scripts/JQuery/jquery-ui.min.js"></script>
    <script src="~/Scripts/JQuery/jquery.tooltipster.min.js"></script>
    <script src="~/Scripts/JQuery/jquery.plugin.min.js"></script>
    <script src="~/Scripts/JQuery/jquery.maxlength.min.js"></script>
    <script src="~/Scripts/Plugins/holder.js"></script>
    <script src="~/Scripts/JQuery/jquery.bar.js"></script>
    <script src="~/Scripts/JQuery/jquery.cookie.js"></script>
    <script src="~/Scripts/Custom/custom.js"></script>
    <script src="~/Scripts/Bootstrap/bootstrap.js"></script>
    <script src="~/Scripts/Coral/jquery.nicescroll.min.js"></script>
    <script src="~/Scripts/Coral/pace.min.js"></script>
    <script src="~/Scripts/Coral/preload.pace.init.js"></script>
    <script src="~/Scripts/Coral/breakpoints.js"></script>
    <script src="~/Scripts/Coral/core.init.js"></script>
    <script src="~/Scripts/Coral/animations.init.js"></script>

    <script src="~/Scripts/jquery.cookiebar.js"></script>
    <script src="~/Scripts/JQuery/jquery.scrollTo.min.js"></script>
    <script src="https://maps.googleapis.com/maps/api/js?key=AIzaSyCCXlTpQmG4pisYLG8WA0duXThg_XiyH6w&sensor=false"></script>
    <script src="~/Scripts/JQuery/jquery-gmaps-latlon-picker.js"></script>
    <script src="~/Scripts/JQuery/spin.min.js"></script>
    <script src="~/Scripts/Coral/image-preview.js"></script>
    <script type="text/javascript" src="//s7.addthis.com/js/300/addthis_widget.js#pubid=ra-537f4d8d0e803558" async="async"></script>
    <script src="~/Scripts/Custom/addthis.js"></script>

    @RenderSection("FooterScripts", false)

    @if (!Request.IsAuthenticated)
    {
        <script src="~/Scripts/Coral/modals.init.js"></script>
        @Html.Partial("~/Views/Modals/Signin.cshtml")

    }
    else
    {
        <script src="~/Scripts/Coral/modals.init.js"></script>
        if (Model.Modals.Count > 0)
        {
            foreach (var modal in Model.Modals)
            {
                <div class="modal fade" id="@modal.Id" style="display: none;" aria-hidden="true">
                    <div class="modal-dialog" style="width: @modal.Width;">
                        @Html.Partial("~/Views/Modals/" + modal.Path, modal.Model)
                    </div>
                </div>
            }
        }
    }
    <!--Google Analytics-->
    <script>
        (function (i, s, o, g, r, a, m) {
            i['GoogleAnalyticsObject'] = r;
            i[r] = i[r] || function () {
                (i[r].q = i[r].q || []).push(arguments);
            }, i[r].l = 1 * new Date();
            a = s.createElement(o),
                m = s.getElementsByTagName(o)[0];
            a.async = 1;
            a.src = g;
            m.parentNode.insertBefore(a, m);
        })(window, document, 'script', '//www.google-analytics.com/analytics.js', 'ga');

        ga('create', 'UA-51665375-1', 'quarinos.com');
        ga('send', 'pageview');

    </script>
    @if (Model.NotificationMessage != null && Model.NotificationMessage.IsEmpty())
    {
        <script language="javascript">
            $(document).ready(function () {

                $(".notify").click(function (e) {
                    e.preventDefault();
                    $("#note").animate({ top: 60 }, 300, null);
                    //for css positioning prblem
                    $("#note").css("position", "fixed");

                    //Hide the bar
                    var $notifyTimer = setTimeout(function () {
                        $("#note").animate({ top: -100 }, 1000, function () {
                            //change back to absolute
                            $("#note").css("position", "absolute");
                        });

                    }, 5000);
                });
                $(".notify").trigger('click');


            });

        </script>
        Model.NotificationMessage = null;
    }
</body>







</html>